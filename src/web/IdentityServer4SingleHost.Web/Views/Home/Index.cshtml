@model IdentityServer4SingleHost.Web.Models.AccountViewModels.LoginViewModel
@{
    ViewBag.Title = "Admin login | IdentityServer4SingleHost";
    Layout = "MasterPages/_AccountLayout";
}


@using Microsoft.AspNetCore.Authentication

<h2>Claims</h2>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

<dl>
    @foreach (var prop in (await Context.AuthenticateAsync()).Properties.Items)
    {
        <dt>@prop.Key</dt>
        <dd>@prop.Value</dd>
    }
</dl>